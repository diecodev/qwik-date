{
  "conventionalCommits.emojiFormat": "emoji",
  "conventionalCommits.scopes": [
    "website",
    "headless",
    "styled",
    "core"
  ],
  "nodejs-testing.extensions": [
    {
      "extensions": [
        "mjs",
        "cjs",
        "js"
      ],
      "parameters": []
    },
    {
      "extensions": [
        "mts",
        "cts",
        "ts"
      ],
      "parameters": [
        "--import",
        "tsx"
      ]
    }
  ],
}